<form class="form-horizontal" id="frm_customer" name="frm_cus" autocomplete="off">
    <div class="breadcrumb-header justify-content-between">
        <div class="card-header pb-0">
            <h5 class="card-title mb-0 pb-0">ที่อยู่จัดส่ง</h5>
        </div>
        <div class="d-flex my-xl-auto right-content">
            @*<h5 class="card-title mb-0 pb-0">ONL/ISR/CPS</h5>*@
        </div>
    </div>
    <div class="border-top my-3"></div>
    <div class="kt-portlet__body ">
        <div class="form-group form-group-sm row d-flex justify-content-center">
            <label class="col-md-1 col-form-label" align="right">ที่อยู่</label>
            <div class="col-md-9">
                <input class="form-control" placeholder="ชื่อบริษัทขนส่งเอกชน" type="text" id="cus_address" name="cus_address" style="margin-top: -3px;" required data-parsley-error-message="ชื่อบริษัทขนส่งเอกชน">
            </div>
        </div>
        <div class="form-group form-group-sm row d-flex justify-content-center">
            <label class="col-1 col-form-label" align="right">จังหวัด  </label>
            <div class="col-3">
                <select class="form-control  select2 provicne" id="provicne" name="provicne" style="width: 100%;" required data-parsley-error-message="กรุณาเลือกจังหวัด">
                    <option value="">Please select..</option>
                </select>
            </div>
            <label class="col-form-label col-1" align="right">อำเภอ</label>
            <div class="col-3">
                <select class="form-control  select2 amphur" id="amphur" name="amphur" style="width: 100%;" required data-parsley-error-message="กรุณาเลือกอำเภอ">
                    <option value="">Please select..</option>
                </select>
            </div>
            <div class="col-2"></div>
        </div>
        <div class="form-group form-group-sm row d-flex justify-content-center">
            <label class="col-form-label col-1" align="right">ตำบล</label>
            <div class="col-3">
                <select class="form-control  select2 district" id="district" name="district" style="width: 100%;" required data-parsley-error-message="กรุณาเลือกตำบล">
                    <option value="">Please select..</option>
                </select>
            </div>
            <label class="col-form-label col-1" align="right">รหัสไปษณีย์</label>
            <div class="col-3">
                <input type="text" class="form-control" id="postcode" name="postcode" required data-parsley-error-message="กรุณากรอกรหัสไปษณีย์">
            </div>
            <label class="col-form-label col-1" align="right">เรื่มต้น</label>
            <div class="col-auto">
                <div class="main-toggle-group-demo" style="padding: 0.375rem 0.75rem;">
                    <div class="main-toggle main-toggle-success default-switches">
                        <span></span>
                    </div>
                </div>
            </div>
        </div>

        <div class="kt-portlet__body ">
            <div class="form-group form-group-sm row d-flex justify-content-center">
                <div class="col-1">
                    <div class="input-group btn-create">
                        <button id="btn-item-create" type="submit" class="btn btn-success btn-with-icon btn-block ">ADD +</button>
                    </div><!-- input-group -->
                    <div class="input-group btn-edit hide">
                        <button id="btn-edit" type="submit" class="btn btn-success btn-with-icon btn-block">Edit</button>
                    </div><!-- input-group -->

                </div>
                <div class="col-1">
                    <div class="input-group">
                        <button id="btn-item_reset" type="reset" class="btn btn-light btn-with-icon btn-block">Cancel</button>
                    </div><!-- input-group -->
                </div>
            </div>
        </div>
    </div>
</form>

<div class="border-top my-3"></div>
<div class="table-responsive mg-t-20" style="overflow: hidden;">
    <table id="tbl-cus-list" class="table table-bordered table-striped table-hover mg-b-0 text-md-nowrap"></table>
</div>